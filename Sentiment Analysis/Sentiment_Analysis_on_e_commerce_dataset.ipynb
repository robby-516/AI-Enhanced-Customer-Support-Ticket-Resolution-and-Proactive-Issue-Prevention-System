        "dataset_url = \"hf://datasets/NebulaByte/E-Commerce_Customer_Support_Conversations/data/train-00000-of-00001-a5a7c6e4bb30b016.parquet\"\n",
        "df = pd.read_parquet(dataset_url)\n",
        "\n",
        "# Preview dataset structure\n",
        "print(\"Dataset Structure:\")\n",
        "print(df.info())\n",
        "print(\"\\nSample Data:\")\n",
        "print(df.head())\n",
        "\n",
        "# **Derive top 3 insights**\n",
        "\n",
        "# Insight 1: Distribution of conversation lengths\n",
        "df['conversation_length'] = df['conversation'].apply(lambda x: len(x.split()) if pd.notnull(x) else 0)\n",
        "conversation_length_distribution = df['conversation_length'].describe()\n",
        "print(\"\\nInsight 1: Conversation Length Distribution\")\n",
        "print(conversation_length_distribution)\n",
        "\n",
        "# Insight 2: Most frequent customer issues (if issue category column exists)\n",
        "if 'issue_category' in df.columns:\n",
