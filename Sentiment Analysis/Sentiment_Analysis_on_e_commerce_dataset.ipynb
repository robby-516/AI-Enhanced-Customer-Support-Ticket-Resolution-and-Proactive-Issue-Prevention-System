        "else:\n",
        "    print(\"\\nInsight 2: Issue category column not found in the dataset.\")\n",
        "\n",
        "# Insight 3: Customer satisfaction distribution (if satisfaction rating column exists)\n",
        "if 'customer_satisfaction' in df.columns:\n",
        "    satisfaction_distribution = df['customer_satisfaction'].value_counts()\n",
        "    print(\"\\nInsight 3: Customer Satisfaction Distribution\")\n",
        "    print(satisfaction_distribution)\n",
        "else:\n",
        "    print(\"\\nInsight 3: Customer satisfaction column not found in the dataset.\")\n",
        "\n",
        "\n",
        "# Perform sentiment analysis using Gemini API\n",
        "def perform_sentiment_analysis(texts, api_key):\n",
        "    url = \"https://api.geminiai.com/v1/models/gemini-1.5-flash/generate\"\n",
        "    headers = {\n",
        "        \"Content-Type\": \"application/json\",\n",
        "        \"Authorization\": f\"Bearer {api_key}\"\n",
        "    }\n",
        "\n",
        "    results = []\n",
        "    for text in texts:\n",
        "        payload = {\n",
        "            \"prompt\": f\"What is the sentiment of the following text?\\n\\n{text}\",\n",
        "            \"max_tokens\": 10,\n",
        "            \"temperature\": 0.0,\n",
        "            \"top_k\": 40,\n",
        "            \"top_p\": 0.95,\n",
        "        }\n",
        "        response = requests.post(url, json=payload, headers=headers)\n",
        "        if response.status_code == 200:\n",
        "            response_json = response.json()\n",
        "            sentiment = response_json[\"response\"].strip()\n",
        "            results.append({\"text\": text, \"label\": sentiment})\n",
        "        else:\n",
        "            raise Exception(f\"Gemini API request failed with status code {response.status_code}: {response.text}\")\n",
        "\n",
        "    return result\n",
        "\n",
        "# Prepare data for sentiment analysis\n",
        "customer_conversations = df['conversation'].dropna().tolist()[:100]  # Limiting to 100 conversations for demo purposes\n",
        "\n",
        "# Perform sentiment analysis\n",
        "api_key = \"Your api"  # Replace with your actual API key\n",
        "try:\n",
        "    sentiment_results = perform_sentiment_analysis(customer_conversations, api_key)\n",
        "    df_sentiment = pd.DataFrame(sentiment_results)\n",
        "    print(\"\\nSentiment Analysis Results:\")\n",
        "    print(df_sentiment.head())\n",
        "except Exception as e:\n",
        "    print(f\"Error performing sentiment analysis: {e}\")\n"
