        "import google.generativeai as genai\n",
        "\n",
        "genai.configure(api_key=\"Your api\")\n",
        "model = genai.GenerativeModel(\"gemini-1.5-flash\")\n",
        "\n",
        "def get_sentiment(text):\n",
        "  response = model.generate_content(f\"\"\"\n",
        "  What is the sentiment of the customer in the following text?\n",
        "\n",
        "  {text}\n",
        "\n",
        "  Respond with a single word: positive, negative,frustrated, or neutral.\n",
        "  \"\"\")\n",
        "  sentiment = response.text.strip()\n",
        "  return sentiment\n",
        "for i in range(1,10):\n",
        "  text = df.conversation[i]\n",
        "  sentiment = get_sentiment(text)\n",
        "  print(f\"True value {df.customer_sentiment[i]} => predicted value  {sentiment}\")"
