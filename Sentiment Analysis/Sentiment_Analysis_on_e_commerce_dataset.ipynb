        "# Import necessary libraries\n",
        "from sklearn.metrics import confusion_matrix,accuracy_score\n",
        "import seaborn as sns\n",
        "import matplotlib.pyplot as plt\n",
        "import google.generativeai as genai\n",
        "import pandas as pd\n",
        "import requests\n",
        "\n",
        "# Load the dataset\n",
        "dataset_url = \"hf://datasets/NebulaByte/E-Commerce_Customer_Support_Conversations/data/train-00000-of-00001-a5a7c6e4bb30b016.parquet\"\n",
        "df = pd.read_parquet(dataset_url)\n",
        "\n",
        "# Preview dataset structure\n",
        "print(\"Dataset Structure:\")\n",
        "print(df.info())\n",
        "\n",
        "# Derive top 3 insights\n",
        "# Insight 1: Distribution of issue complexities\n",
        "issue_complexity_distribution = df['issue_complexity'].value_counts()\n",
        "print(\"\\nInsight 1: Issue Complexity Distribution\")\n",
        "print(issue_complexity_distribution)\n",
        "\n",
        "# Insight 2: Correlation between agent experience level and issue complexity\n",
        "agent_experience_correlation = df.groupby(['agent_experience_level', 'issue_complexity']).size().unstack(fill_value=0)\n",
        "print(\"\\nInsight 2: Agent Experience Level vs Issue Complexity\")\n",
        "print(agent_experience_correlation)\n",
        "\n",
        "# Insight 3: Customer sentiment distribution across issue categories\n",
        "sentiment_category_distribution = df.groupby(['customer_sentiment', 'issue_category']).size().unstack(fill_value=0)\n",
        "print(\"\\nInsight 3: Customer Sentiment Distribution by Issue Category\")\n",
        "print(sentiment_category_distribution)\n",
        "\n",
        "\n",
        "\n",
        "# Function to get sentiment\n",
        "def get_sentiment(text):\n",
        "    response = model.generate_content(f\"\"\"\n",
        "    What is the sentiment of the customer in the following text?\n",
        "\n",
        "    {text}\n",
        "\n",
        "    Respond with a single word: positive, negative, frustrated, or neutral.\n",
        "    \"\"\")\n",
        "    sentiment = response.text.strip().lower()  # Normalize the sentiment (case insensitive)\n",
        "    print(f\"Predicted sentiment: '{sentiment}'\")  # Debugging line\n",
        "    return sentiment\n",
        "\n",
        "# Collect true and predicted sentiments\n",
        "true_sentiments = []\n",
        "predicted_sentiments = []\n",
        "\n",
        "# Loop through the dataset to collect sentiments\n",
        "for i in range(1, 10):\n",
        "    text = df.conversation[i]\n",
        "    sentiment = get_sentiment(text)\n",
        "    true_sentiments.append(df.customer_sentiment[i].lower())  # Normalize to lowercase\n",
        "    predicted_sentiments.append(sentiment)\n",
        "\n",
        "# Generate confusion matrix\n",
        "cm = confusion_matrix(true_sentiments, predicted_sentiments, labels=['positive', 'negative', 'frustrated', 'neutral'])\n",
        "accuracy = accuracy_score(true_sentiments, predicted_sentiments)\n",
        "print(f\"Accuracy Score is {accuracy*100}\")\n",
        "\n",
        "# Visualize confusion matrix\n",
        "plt.figure(figsize=(8, 6))\n",
        "sns.heatmap(cm, annot=True, fmt='d', cmap='Blues', xticklabels=['positive', 'negative', 'frustrated', 'neutral'], yticklabels=['positive', 'negative', 'frustrated', 'neutral'])\n",
        "plt.xlabel('Predicted Sentiment')\n",
        "plt.ylabel('True Sentiment')\n",
        "plt.title('Confusion Matrix')\n",
        "plt.show()\n",
        "\n"
      ],
